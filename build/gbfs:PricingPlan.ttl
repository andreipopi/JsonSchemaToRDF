@prefix gbfs: <https://w3id.org/gbfs/terms>.
@prefix schema: <http://schema.org/#>.
@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vann: <http://purl.org/vocab/vann/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix jsonsc: <https://www.w3.org/2019/wot/json-schema#>.
@prefix airs: <https://raw.githubusercontent.com/airs-linked-data/lov/latest/src/airs_vocabulary.ttl#>.
@prefix vso: <http://purl.org/vso/ns#>.
@prefix dbpedia-owl: <http://dbpedia.org/ontology/>.

<https://w3id.org/gbfs/vocabularies/plans> rdf:type foaf:Document;
    rdfs:comment "Describes the pricing schemes of the system."@en;
    vann:preferredNamespaceUri "https://w3id.org/gbfs/vocabularies/plans#";
    dcterms:creator <https://pietercolpaert.be/#me>, <https://www.linkedin.com/in/andrei-popescu/>.
<https://pietercolpaert.be/#me> rdf:type foaf:Person;
    foaf:mbox "mailto:pieter.colpaert@imec.be";
    foaf:name "Pieter Colpaert".
gbfs:PricingPlan rdf:type rdfs:Class.
gbfs:plan_id rdf:type rdf:Property;
    rdfs:label "Identifier of a pricing plan in the system."@en.
gbfs:currency rdf:type rdf:Property;
    rdfs:label "Currency used to pay the fare in ISO 4217 code."@en.
gbfs:price rdf:type rdf:Property;
    rdfs:label "Fare price."@en.
gbfs:is_taxable rdf:type rdf:Property;
    rdfs:label "Will additional tax be added to the base price?"@en;
    rdfs:range xsd:boolean.
gbfs:per_km_pricing rdf:type rdf:Property;
    rdfs:label "Array of segments when the price is a function of distance travelled, displayed in kilometers (added in v2.1-RC2)."@en;
    rdfs:range gbfs:Per_km_pricing.
gbfs:per_min_pricing rdf:type rdf:Property;
    rdfs:label "Array of segments when the price is a function of time travelled, displayed in minutes (added in v2.1-RC2)."@en;
    rdfs:range gbfs:Per_min_pricing.
gbfs:surge_pricing rdf:type rdf:Property;
    rdfs:label "Is there currently an increase in price in response to increased demand in this pricing plan? (added in v2.1-RC2)"@en;
    rdfs:range xsd:boolean.
gbfs:Per_km_pricing rdfs:label "Array of segments when the price is a function of distance travelled, displayed in kilometers (added in v2.1-RC2)."@en;
    rdf:type rdfs:Class.
gbfs:start rdf:type rdf:Property;
    rdfs:label "Number of kilometers that have to elapse before this segment starts applying (added in v2.1-RC2)."@en;
    rdfs:range xsd:integer.
gbfs:rate rdf:type rdf:Property;
    rdfs:label "Rate that is charged for each kilometer interval after the start (added in v2.1-RC2)."@en.
gbfs:interval rdf:type rdf:Property;
    rdfs:label "Interval in kilometers at which the rate of this segment is either reapplied indefinitely, or if defined, up until (but not including) end kilometer (added in v2.1-RC2)."@en;
    rdfs:range xsd:integer.
gbfs:end rdf:type rdf:Property;
    rdfs:label "The kilometer at which the rate will no longer apply (added in v2.1-RC2)."@en;
    rdfs:range xsd:integer.
gbfs:Per_min_pricing rdfs:label "Array of segments when the price is a function of time travelled, displayed in minutes (added in v2.1-RC2)."@en;
    rdf:type rdfs:Class.
